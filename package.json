{
  "type": "module",
  "name": "TomHuangCN.github.io",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "post-build": "node scripts/post-build.js",
    "build:cdn": "pnpm run build && pnpm run post-build",
    "preview": "vite preview",
    "deploy": "pnpm run build:cdn && cp dist/index.html dist/404.html && gh-pages -d dist",
    "prepare": "husky install",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "check": "pnpm run lint && tsc --noEmit",
    "cache-bust": "node scripts/cache-bust.js",
    "cache-restore": "node scripts/restore-cache.js"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react": "^7.37.5",
    "gh-pages": "^6.3.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3",
    "vite": "^5.4.19"
  },
  "dependencies": {
    "jszip": "^3.10.1",
    "lunar-javascript": "^1.7.3",
    "lz-string": "^1.5.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.7.1",
    "roughjs": "^4.6.6",
    "sanitize.css": "^13.0.0"
  }
}
